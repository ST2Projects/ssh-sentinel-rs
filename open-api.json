{
  "openapi": "3.0.0",
  "info": {
    "title": "ssh-sentinel-rs",
    "version": "0.1.0"
  },
  "paths": {
    "/hello": {
      "get": {
        "operationId": "hello",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "404": {
            "description": ""
          }
        },
        "security": [
          {
            "ApiKeyAuth": []
          }
        ]
      }
    },
    "/signKey": {
      "post": {
        "operationId": "signKey",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/KeySignRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/KeySignResponse"
                }
              }
            }
          }
        },
        "security": [
          {
            "ApiKeyAuth": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "KeySignResponse": {
        "type": "object",
        "required": [
          "signed_key"
        ],
        "properties": {
          "signed_key": {
            "type": "string"
          }
        }
      },
      "KeySignRequest": {
        "type": "object",
        "required": [
          "pub_key"
        ],
        "properties": {
          "pub_key": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "ApiKeyAuth": {
        "description": "Requires an API Key access",
        "type": "apiKey",
        "name": "x-api-key",
        "in": "header"
      }
    }
  }
}